<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="360.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/18">
    <children>
        <HBox alignment="CENTER">
            <children>
                <TextField alignment="CENTER" prefHeight="50.0" prefWidth="260.0" promptText="Correo electrónico">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </TextField>
            </children>
            <VBox.margin>
                <Insets bottom="20.0" left="20.0" right="20.0"/>
            </VBox.margin>
        </HBox>
        <HBox alignment="CENTER">
            <children>
                <TextField alignment="CENTER" prefHeight="50.0" prefWidth="260.0" promptText="Contraseña">
                    <HBox.margin>
                        <Insets bottom="20.0"/>
                    </HBox.margin>
                    <font>
                        <Font size="18.0"/>
                    </font>
                </TextField>
            </children>
            <VBox.margin>
                <Insets left="20.0" right="20.0"/>
            </VBox.margin>
        </HBox>
        <Button mnemonicParsing="false" prefHeight="60.0" prefWidth="140.0" text="Ingresar">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
    </children>
</VBox>
